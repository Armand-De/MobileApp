<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobile_contentsapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Recipe.Recipe_Activity"></activity>
        <activity android:name=".Community.Commu_Activity" />
        <activity
            android:name=".Recipe.Insert_Activity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".Recipe.Main_Activity" />
        <activity android:name=".Login.Signup_Activity" />
        <activity android:name=".Login.Sign_in_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>